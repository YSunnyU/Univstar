<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sunny.univstar.view.teachertype.activity.TeacherFollowerActivity">
    <RelativeLayout
        android:gravity="center_vertical"
        android:id="@+id/reaaa_2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true">
        <ImageView
            android:layout_marginLeft="10dp"
            android:id="@+id/teacher_follower_return"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@mipmap/register_return" />

        <TextView
            android:id="@+id/teacher_fan_title"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="作业详情"
            android:textColor="#0073ff"
            android:textSize="18sp" />
    </RelativeLayout>
    <ListView
        android:layout_below="@+id/reaaa_2"
        android:id="@+id/teacher_follower_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>
</RelativeLayout>
